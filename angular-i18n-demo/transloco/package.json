{
  "name": "angular-i18n-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prettier": "prettier --write .",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "bracketSameLine": true,
    "printWidth": 80,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.2.4",
    "@angular/compiler": "20.2.4",
    "@angular/core": "20.2.4",
    "@angular/forms": "20.2.4",
    "@angular/platform-browser": "20.2.4",
    "@angular/router": "20.2.4",
    "@jsverse/transloco": "7.6.1",
    "@jsverse/transloco-locale": "7.0.1",
    "@jsverse/transloco-messageformat": "7.0.1",
    "@jsverse/transloco-persist-lang": "7.0.2",
    "@jsverse/transloco-persist-translations": "7.0.1",
    "@jsverse/transloco-preload-langs": "7.0.1",
    "ngx-transloco-markup": "6.0.2",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular/build": "20.2.2",
    "@angular/cli": "20.2.2",
    "@angular/compiler-cli": "20.2.4",
    "@jsverse/transloco-keys-manager": "7.0.2",
    "@jsverse/transloco-optimize": "7.0.1",
    "@jsverse/transloco-validator": "7.0.1",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  }
}
